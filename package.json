{
  "main": "src/frontend/index.js",
  "name": "windmill-dashboard-react",
  "version": "0.4.0",
  "description": "A multi theme, completely accessible, with components and pages examples, (almost) ready for production dashboard.",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss -i build src/frontend/assets/css/tailwind.css -o src/frontend/assets/css/tailwind.output.css --watch",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/frontend/assets/css/tailwind.css -o src/frontend/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "set NODE_PATH=src/frontend && react-scripts start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "^0.3.1",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "faker": "4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "multer": "^2.0.1",
    "pg": "^8.14.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.5",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-hook-form": "^7.55.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "4.4.1",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "@tailwindcss/vite": "^4.1.3",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.14.0",
    "autoprefixer": "^9.8.8",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss": "^8.5.3",
    "postcss-cli": "7.1.1",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "^1.4.6",
    "ts-node": "^10.9.2",
    "typescript": "4.6"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
